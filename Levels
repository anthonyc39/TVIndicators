// Updated implementation for Levels file

// Function to extend and dynamically adjust lines and labels based on session timing
function adjustLinesAndLabels() {
    const now = new Date();
    const endSession = new Date();
    endSession.setHours(16, 15, 0); // Set to 4:15 PM
    const isAfterSession = now > endSession;

    if (isAfterSession) {
        // Clear all lines and labels
        clearLinesAndLabels(); 
    } else {
        // Logic to extend and adjust lines and labels until 4:15 PM
        extendLinesAndLabels();
    }
}

// Function to ensure midnight open line is solid
function setMidnightOpenLine() {
    const midnightOpenLine = getMidnightOpenLine();
    midnightOpenLine.style.borderStyle = 'solid'; // Set solid line
}

// Function to handle invalidation of Asia session levels during London session
function handleAsiaSessionInvalidation() {
    if (asiaSessionLevelsInvalidated()) {
        // Switch from solid to dashed lines for Asia levels
        setAsiaLevelsToDashed();
        // Apply strikethrough on labels
        applyStrikethroughOnLabels();
    }
}

// Call functions to apply the changes
adjustLinesAndLabels();
setMidnightOpenLine();
handleAsiaSessionInvalidation();

